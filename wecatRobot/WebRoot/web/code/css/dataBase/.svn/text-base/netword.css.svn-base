.redColor {
  color: red;
}
.borderBottom {
  border-bottom: 1px solid #ccc;
}
.pBottotm {
  padding-bottom: 20px;
}
.w180 {
  width: 180px;
}
.container-fluid {
  padding: 0;
}
.contentArea {
  padding-top: 12px;
  color: #333;
  font-size: 16px;
  padding-bottom: 30px;
}
.networkPage {
  background-color: #fff;
  padding-left: 46px;
  padding-right: 46px;
}
.networkPage .inquiry {
  width: 100%;
  text-align: center;
  padding-bottom: 24px;
  margin-top: 32px;
  border-bottom: 1px solid #ccc;
}
.networkPage .inquiry > div {
  display: inline-block;
  position: relative;
}
.networkPage .inquiry > div span {
  position: absolute;
  width: 16px;
  height: 16px;
  background: url("../../images/icon_search_blue.png") no-repeat center;
  left: 14px;
  top: 50%;
  margin-top: -8px;
}
.networkPage .inquiry > div input {
  width: 366px;
  height: 34px;
  -webkit-border-radius: 17px;
  -moz-border-radius: 17px;
  border-radius: 17px;
  border: 1px solid #ccc;
  padding-left: 42px;
  background-color: #fff;
  line-height: 34px;
  font-size: 14px;
}
.inquiryBtnCom {
  background-color: #1c9bd9;
  width: 100px;
  height: 34px;
  color: #fff;
  -webkit-border-radius: 17px;
  -moz-border-radius: 17px;
  border-radius: 17px;
  margin-left: 12px;
  font-size: 14px;
}
.iconBtn {
  width: 100px;
  height: 32px;
  -webkit-border-radius: 16px;
  -moz-border-radius: 16px;
  border-radius: 16px;
  background-color: #1c9bd9;
  color: #fff;
}
.iconBtn span {
  display: inline-block;
  margin-right: 4px;
  vertical-align: middle;
  margin-top: -4px;
}
.addBtn span {
  width: 16px;
  height: 16px;
  background: url("../../images/icon_add_white.png") no-repeat center;
}
.networkPage > div:last-child .addBtnBox {
  text-align: right;
  margin-top: 18px;
  margin-bottom: 28px;
}
.networkList > ul {
  border: 1px solid #ccc;
}
.networkList > ul .row {
  margin: 0;
  line-height: 34px;
  border-top: 1px solid #ccc;
  font-size: 14px;
}
.networkList > ul .row div {
  padding: 0;
  text-align: center;
}
.networkList > ul .row .lookBtn,
.networkList > ul .row .editBtn {
  color: #0098E3;
  margin-right: 4px;
}
.networkList > ul .row .deleteBtn {
  color: #D1D1D1;
  margin-right: 4px;
}
.networkList > ul .row .downUpicon {
  display: inline-block;
  width: 16px;
  height: 16px;
  background: url("../../images/icon_drop_down_edit.png") no-repeat center;
  margin-left: 20px;
  cursor: pointer;
}
.networkList > ul .row .jiedaoIcon {
  display: inline-block;
  width: 21px;
  height: 14px;
  background: url("../../images/jiedao.jpg") no-repeat center;
  margin-left: 20px;
}
.networkList > ul .row .pianquIcon {
  display: inline-block;
  width: 21px;
  height: 16px;
  background: url("../../images/pianqu.jpg") no-repeat center;
  margin-left: 20px;
}
.networkList > ul .row .wangluoIcon {
  display: inline-block;
  width: 21px;
  height: 16px;
  background: url("../../images/wangge.jpg") no-repeat center;
  margin-left: 20px;
}
.networkList > ul .row .lookIcon,
.networkList > ul .row .editIcon,
.networkList > ul .row .deleteIcon {
  display: inline-block;
  width: 22px;
  height: 14px;
  margin-right: 2px;
  vertical-align: middle;
}
.networkList > ul .row .addPianQuBtn,
.networkList > ul .row .addWangLuoBtn {
  display: inline-block;
  width: 100px;
  height: 20px;
  line-height: 20px;
  background-color: #0098E3;
  color: #fff;
  border-radius: 10px;
  text-align: center;
  vertical-align: middle;
  cursor: pointer;
}
.networkList > ul .row .addWangLuoBtn {
  width: 120px;
}
.networkList > ul .row .addPianQuBtn .addIcon,
.networkList > ul .row .addWangLuoBtn .addIcon {
  display: inline-block;
  width: 16px;
  height: 16px;
  vertical-align: middle;
  margin-top: -3px;
  background: url("../../images/icon_add_white.png") no-repeat center;
}
.networkList > ul .row .lookIcon {
  background: url("../../images/icon_look.png") no-repeat center;
}
.networkList > ul .row .editIcon {
  background: url("../../images/icon_edit.png") no-repeat center;
}
.networkList > ul .row .deleteIcon {
  background: url("../../images/icon_delete.png") no-repeat center;
}
.networkList > ul .row.huiColor {
  background-color: #f2f2f2;
  border-top: 1px solid #fff;
}
.networkList > ul .listTitle {
  background-color: #E0F7FF;
}
.move-enter-active,
.move-leave-active {
  transition: opacity ease-out 0.3s;
  /*transform ease-out .5s;*/
}
.move-enter,
.move-leave {
  /*transform: translateX(100px);*/
  opacity: 0;
}
.mask {
  position: fixed;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  background-color: rgba(0, 0, 0, 0.2);
  z-index: 99;
}
.mask > div:first-child {
  margin: 100px auto;
  width: 1000px;
  padding-bottom: 40px;
  background-color: #fff;
  display: block;
  -webkit-border-radius: 10px;
  -moz-border-radius: 10px;
  border-radius: 10px;
}
.mask > div:first-child .modelTop {
  padding-left: 28px;
  line-height: 50px;
  border-bottom: 1px solid #ccc;
  font-size: 18px;
}
.mask > div:first-child .modelTop span {
  position: absolute;
  right: 20px;
  width: 22px;
  height: 22px;
  background: url("../../images/icon_cancel.png") no-repeat center;
  top: 50%;
  margin-top: -11px;
  cursor: pointer;
}
.mask > div:first-child .modelContent {
  padding: 10px 50px 10px 50px;
  font-size: 16px;
}
.mask > div:first-child .modelContent .row > div {
  padding: 0;
  margin-top: 20px;
}
.mask > div:first-child .modelContent .row > div > span {
  display: inline-block;
}
.mask > div:first-child .modelContent .row > div > input {
  border: 1px solid #ccc;
  -webkit-border-radius: 4px;
  -moz-border-radius: 4px;
  border-radius: 4px;
  height: 30px;
}
.mask > div:first-child .modelContent .row > div .huaFen {
  width: 80%;
}
.mask > div:first-child .modelContent .row > div .radio {
  display: inline-block;
}
.mask > div:first-child .modelContent .row > .pianQuMiaoShu input {
  width: 80%;
}
.mask > div:first-child .saveBtn {
  width: 132px;
  height: 36px;
  -webkit-border-radius: 18px;
  -moz-border-radius: 18px;
  border-radius: 18px;
  background-color: #1c9bd9;
  line-height: 36px;
  text-align: center;
  color: #fff;
  margin: 30px auto;
  cursor: pointer;
}
.mask p {
  padding-left: 40px;
  font-size: 16px;
  font-family: "Microsoft YaHei";
  color: #666;
}
.mask p span {
  margin-right: 30px;
}
#allmap {
  width: 100%;
  height: 285px;
}
