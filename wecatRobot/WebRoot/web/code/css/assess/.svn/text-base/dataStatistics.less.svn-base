body{
  background-color: #f2f2f2;
  >.top{
    ul.oneMenu{
      >li{
        margin-right: 20px;
       ol{
         width: 100%;
         box-shadow: 8px 8px 30px #666666;;
         position: absolute;
         display: none;
         z-index:5;
         >li:first-child{
           border-top:1px solid #0098e4;
         }
         >li{
           height: 34px;
           line-height: 34px;
           background-color: #007cb9;
           border-bottom:1px solid #0098e4;
           >a{
             text-align: center;
             display: block;
             width: 100%;
             height: 100%;
             color: #ffffff;
             text-decoration: none;
           }
         }
         >li:hover{
           background-color: #0098e4;
         }
       }
      }
      >li.active{
        background-color: #007cb9
      }
    }
  }
  >.content{
    //background-color: #fff;
    >.path{
      padding-left:30px;
      line-height:46px;
      background-color: #fff;
      margin:20px 0 12px 0;
      >.el-breadcrumb{
        line-height:46px;
      }
    }
    >.content2{
      background-color: #fff;
      //padding-bottom: 50px;
      position: relative;
      padding:0 46px 50px 46px;
       >h3{
         line-height: 70px;
         padding-left: 50px;
         font-size: 20px;
         position: relative;
         >i{
           position: absolute;
           width: 3px;
           height: 15px;
           top: 28px;
           left: 35px;
           background-color: #1b9ad8;
         }
       }
      >.select{
        height: 68px;
        padding:20px 0 0 20px;
        background-color: #f2f2f2;
        >div.el-input{
          display: inline-block;
          width: 226px;
          margin-right: 20px;
        }
        >div.el-select{
          width: 126px;
          margin-left: 15px;
          margin-right: 25px;
        }
      }
      >.opt{
        text-align: center;
        height: 100px;
        padding-top:25px;
        >button{
          position: relative;
          border-radius: 30px;
          font-size: 18px;
          width: 125px;
          height: 36px;
          background-color: #0098e4;
          color: #ffffff;
          >i{
            position: absolute;
            width: 14px;
            height: 14px;
            left: 20px;
            top: 10px;
          }
        }
      }
      >.tableBox{
          background-color: #fff;
          padding: 10px 0 30px 0;
        .companyLis{
          width: 100%;
          tbody{
            width: 100%;
          }
        }
        .companyList th,
        .companyList td{
          text-align: center;
          font-size: 14px;
          border: 1px solid #dde0df;
          padding-left: 14px;
          padding-right: 14px;
        }
        .companyList th{
          background-color: #e0f8ff;
          height: 66px;
          color: #666;
          font-weight: 400;
        }
        .companyList td{
          height: 42px;
          color: #666;
        }
        .heightLight td{
          background-color: #f9f9f9;
        }
        .companyList td.look{
          color: #0098e4;
          cursor: pointer;
        }
        .companyList td.look span{
          display: inline-block;
          width: 17px;
          height: 13px;
          background: url("../../images/icon_look.png") no-repeat center;
          margin-right: 6px;
          vertical-align: middle;
          margin-top: -2px;
        }

      }
      >.el-pagination{
        float: right;
        margin-right: 35px;
      }
    }
  }
}