<script>

    var vm = new Vue({
        el:"#content",
        data:{
            index:"",
            index2:"",
            radio: '1',//单选框
            areaName:"",
            areaId:"",
            miaoShu:"",
            lookJieDaoModel:false,
            lookPianQuModel:false,
            lookWangLuoModel:false,
            editJieDaoModel:false,
            editPianQuModel:false,
            editWangLuoModel:false,
            addPianQuModel: false,//添加片区
            addWangLuoModel:false,//添加网络
            jieDaoList:[
                {
                    pianQuList:[]
                },
            ],
        },
        methods:{
            onSubmit:function(){
                console.log(123)
            },
            lookJieDao:function(index){
                this.index = index;
                this.lookJieDaoModel = true
            },
            lookPianqu:function(index,index2){
                this.index = index;
                this.index2 = index2;
                this.lookPianQuModel = true;
            },
            lookWangLuo:function(index,index2,index3){
                this.index = index;
                this.index2 = index2;
                this.index3 = index3;
                this.lookWangLuoModel = true;
            },
            editJieDao:function(index){
                this.index = index;
                this.editJieDaoModel = true;
            },
            editJieDaoSave:function(){
                this.editJieDaoModel = false;
            },
            editPianQu:function(index,index2){
                this.index = index;
                this.index2 = index2;
                this.editPianQuModel = true;
            },
            editPianQuSave:function(){
                this.editPianQuModel = false;
            },
            editWangGe:function(index,index2,index3){
                this.index = index;
                this.index2 = index2;
                this.index3 = index3;
                this.editWangLuoModel = true;
            },
            editWangGeSave:function(){
                this.editWangLuoModel = false;
            },
            addJieDao:function(){
                this.jieDaoList.push({ pianQuList:[]})
            },
            addPianQu: function(index){
                this.addPianQuModel = true;
                this.index = index;
            },
            addPianQuSave:function(){
                this.addPianQuModel = false;
                var obj = {
                    areaName:this.areaName,
                    areaId: this.areaId,
                    miaoShu:this.miaoShu,
                    wangGeList:[]
                };
                this.jieDaoList[this.index].pianQuList.push(obj)

            },
            addWangGe:function(index,index2){
                this.addWangLuoModel = true;
                this.index = index;
                this.index2 = index2;
            },
            addWangGeSave:function(){
                this.addWangLuoModel = false;
                var obj = {
                    areaName:this.areaName,
                    areaId: this.areaId,
                    miaoShu:this.miaoShu
                };
                this.jieDaoList[this.index].pianQuList[this.index2].wangGeList.push(obj)
            },
            deletJieDao:function(index,index2){

                this.jieDaoList.splice(index, 1);

                console.log(this.jieDaoList)
            },
            deletePianQu:function(index,index2){
                this.jieDaoList[index].pianQuList.splice(index2, 1);
            },
            deletWangGe:function(index,index2,index3){
                this.jieDaoList[index].pianQuList[index2].wangGeList.splice(index3, 1);
            },
            dropDown:function(e){
               var idStatus = $(e.currentTarget).attr("data-id");
                if(idStatus==0){
                    $(e.currentTarget).parent().parent().siblings().slideDown(200);
                    $(e.currentTarget).css({
                        background: 'url("../../images/icon_pack-up_edit.png") no-repeat center',
                    }).attr("data-id","1")
                }else{
                    $(e.currentTarget).parent().parent().siblings().slideUp(200);
                    $(e.currentTarget).css({
                        background: 'url("../../images/icon_drop_down_edit.png") no-repeat center',
                    }).attr("data-id","0")
                }
            }



          }
    });
//    地图
    var map = new BMap.Map("allmap");
    map.centerAndZoom(new BMap.Point(116.404, 39.915), 15);
    map.enableScrollWheelZoom();

    var polygon = new BMap.Polygon([
        new BMap.Point(116.387112,39.920977),
        new BMap.Point(116.385243,39.913063),
        new BMap.Point(116.394226,39.917988),
        new BMap.Point(116.401772,39.921364),
        new BMap.Point(116.41248,39.927893)
    ], {strokeColor:"blue", strokeWeight:2, strokeOpacity:0.5});  //创建多边形

    map.addOverlay(polygon);
    polygon.enableEditing();
    polygon.addEventListener("lineupdate",function(e){
        console.log(e.currentTarget.po);
    });
</script>
