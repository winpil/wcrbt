@charset "UTF-8";
body{background-color: #f4f4f4; color: #555;}
.Header{
	width: 100%;
	height: 48px; 
	/*background-color: rgb(255, 68, 0); */
	background-color: #f3093f;
	color: #fff;
	overflow: hidden;
    font-size: 18px;
    position: fixed;
    left: 0;
    top:0;
    z-index: 2;
}
.Header .ewm,.Header .goBack{
	display: -webkit-inline-flex;
    -webkit-flex-direction: column;
    -webkit-justify-content: center;
    -webkit-align-items: center;
    display: -webkit-inline-box;
    -webkit-box-orient: vertical;
    -webkit-box-align: center;
    -webkit-box-pack: center;
    -webkit-flex-flow: column;
    display: -ms-inline-flexbox;
    -ms-flex-direction: column;
    -ms-flex-pack: center;
    -ms-flex-align: baseline;
    display: inline-flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    height: 100%;
    margin-left: 5px;
}
.Header .goBack{
	width: 40px;
	height: 100%;
	color: #fff;
	padding: 8px 12px;
}
.Header .ewm i{
	font-size: 20px;
}
.Header .ewm span{
	font-size: 12px;
    font-weight: normal;
}
.Header .search{
	position: absolute;
	left: 50%;
	top:0;
	width: 49.33%;
	height: 100%;
	text-align: center;
    font-weight: bold;
    overflow: hidden;
    text-overflow: ellipsis;
    color: inherit;
    white-space: nowrap;
    padding: 12px 0;
    transform:translate(-50%,0);
}
.Header .search_box{
	padding: 10px 0;
}
.Header .search_box input{
	display: block;
	float: left;
	width: 100%;
	height: 30px;
    padding: 5px 10px;
    line-height: 1.5;
    border-radius: 3px;
    border-bottom: 1px solid #eee;
    font-size: 16px;
    padding-right: 32px;
    /*background-color: rgb(255, 68, 0);*/
	background-color:#f3093f;
    color: #fff;
}
.Header .search_box i{
	float: left;
    width: 30px;
    height: 30px;
    line-height: 30px;
    margin-left: -30px;
}
.Header .btn{
	float: right;
	margin-bottom: 0;
    font-weight: normal;
    text-align: center;
    vertical-align: middle;
    touch-action: manipulation;
    cursor: pointer;
    background-image: none;
    border: 1px solid #f0ad4e;
    white-space: nowrap;
    padding: 8px 12px;
    font-size: 14px;
    line-height: 1.42857143;
    border-radius: 4px;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
    margin: 5px;
    color: #ffffff;
    background-color: #f0ad4e;
}
.Header .search span{
	font-size: 16px;
}
.Header .search b{
	display: block;
	height: 3px;
    border: 1px solid rgb(255, 255, 255);
    border-top: none;
}
.Header .close_search{
	position: absolute;
	left: 74.5%;
	top: 50%;
	transform:translate(-100%,-50%);
	-webkit-transform:translate(-100%,-50%);
}
